<table style="width: 30%;float: right;">
	<thead> 
	<tr>    
		<th width="50%">Name</th>
		<th width="25%">Interviews</th>
		<th width="25%">Appointments</th>
	</tr>   
	</thead>
	<tbody>
	<% @agents_interviews.values.each do |v| %>
		<tr>
			<td><%= v[:name] %></td>
			<td><%= v[:interviews] %></td>
			<td><%= v[:appointments] %></td>
		</tr>
	<% end %>
	</tbody>
</table>
